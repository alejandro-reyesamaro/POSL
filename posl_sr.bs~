		echo "----$inst $inst 3 - 40  -----" >> $file3
		echo "----$inst $inst 3 - 40  -----" >> $file4
		for (( i = 0; i < $runs; i++ ))
		do
			mpiexec.mpich2 -np 40 ./bin/POSL golfers $inst-$inst-3 ./POSL_golfers_SS.posl >> $file1
			mpiexec.mpich2 -np 40 ./bin/POSL golfers $inst-$inst-3 ./POSL_golfers_AS.posl >> $file2
			mpiexec.mpich2 -np 40 ./bin/POSL golfers $inst-$inst-3 ./POSL_golfers_ASrhoSS.posl >> $file3
			mpiexec.mpich2 -np 40 ./bin/POSL golfers $inst-$inst-3 ./POSL_golfers_ASuSS.posl >> $file4
		done
		echo "----END-----" >> $file1
		w >> $file1
