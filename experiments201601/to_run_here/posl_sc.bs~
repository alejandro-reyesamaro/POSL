runs=1

files_results=golomb_12_85[tabu].pres;
strs=golomb.posl

cores=4

#echo "---- 12-85 : $cores  -----" >> $files_results
		
for (( i = 0; i < $runs; i++ ))
do
	mpiexec.mpich -np $cores /home/reyesamaro-a/Documents/POSL/POSL/bin/POSL golomb 9-44 $strs #>> $files_results
done
