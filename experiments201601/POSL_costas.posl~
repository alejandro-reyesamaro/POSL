St_ca000 := cStrategy
	oModule: OM_S, OM_R, OM_V, OM_SS, OM_D;
{
<S>
	OP.|->
	OM_S
	<S>
		OP.Cyc ( BE.LoopBnd 7000 )
		<S> 	OP.|->
				OM_R
				<S> 	OP.Cyc ( BE.LoopBnd 3 )
						<S> 	OP.|->
								<S> 	OP.|->
										<S> 	OP.|-> 
												<S> OP.|-> OM_V OM_SS </S> 
												OM_D 
										</S> OMS.IterCounter
								</S> OMS.TimeCounter
						</S>
				</S>
		</S>
	</S> 
</S>
};

