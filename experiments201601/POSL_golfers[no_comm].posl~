St_a000 := cStrategy
	oModule: OM_S, OM_V, OM_SS, OM_D;
{
<S>
	OP.Cyc ( BE.LoopBnd 50 )
	<S> 	OP.|->
			OM_S
			<S> 	OP.Cyc ( BE.LoopBnd 1000) //( BE.AND ( BE.LoopBnd 3000 , BE.SCI 100 ) )
					<S> 	OP.|->
							<S> 	OP.|->
									<S> 	OP.|-> 
											<S> OP.|-> OM_V OM_SS </S> 
											OM_D 
									</S> OMS.IterCounter
							</S> OMS.TimeCounter
					</S>
			</S>
	</S> 
</S>
};

St_a001 := cStrategy
