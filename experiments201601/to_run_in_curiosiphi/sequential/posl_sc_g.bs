runs=35

p=(11 5 8 9);
g=(7 3 4 4);
w=(5 7 7 8);

for (( j = 0; j < 4; j++ ))
do	
	echo "---- ${p[j]}-${g[j]}-${w[j]} : SEQ -----" >> g.pres
	for (( i = 0; i < $runs; i++ ))
	do
		mpiexec.mpich2 -np 1 ./bin/POSL golfers ${p[j]}-${g[j]}-${w[j]} g.posl >> g.pres
	done
	echo "----END-----" >> g.pres
	echo "   " >> g.pres
done


