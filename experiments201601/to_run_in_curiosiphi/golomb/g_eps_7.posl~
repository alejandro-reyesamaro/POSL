St_g01 := cStrategy
        oModule: OM_S, OM_V, OM_SS, OM_D;
{
<S>
    OP.Cyc ( BE.LoopBnd 300 )
    <S>
        OP.|->
        OM_S
        <S>
            OP.Cyc ( BE.LoopBnd 200 )
            <S>
                OP.|->
                <S>
                    OP.|->
                    <S>
                        OP.|->
                        <S>
                            OP.|->
                            OM_V
                            <S>
                                OP.? ( BE.SCI 25 )
                                OM_SS
                                OM_SS.Rand
                            </S>
                        </S>
                        OM_D
                    </S> OMS.IterCounter
                </S> OMS.TimeCounter
            </S>
        </S>
    </S>
</S>
};

St_g02 := cStrategy
    oModule: OM_S, OM_V, OM_SS, OM_D;
{
<S>
    OP.Cyc ( BE.LoopBnd 300 )
    <S>
        OP.|->
        OM_S
        <S>
            OP.|->
            <S>
                OP.Cyc ( BE.LoopBnd 200 )
                <S>
                    OP.|->
                    <S>
                        OP.|->
                        <S>
                            OP.|->
                            <S>
                                OP.|->
                                OM_V
                                <S>
                                    OP.? ( BE.SCI 25 )
                                    OM_SS
                                    OM_SS.Rand
                                </S>
                            </S>
                            OM_D
                        </S> OMS.IterCounter
                    </S> OMS.TimeCounter
                </S>
            </S>
            OM_R.Tabu
        </S>
    </S>
</S>
};

St_g01COMM := cStrategy
    oModule: OM_S, OM_V, OM_SS, OM_D;
    oChannel: OCh;
{
<S>
    OP.Cyc ( BE.LoopBnd 300 )
    <S>
        OP.|->
        <S>
            OP.First
            <S> OP.|-> OCh (Ch1) OM_R.Tabu </S>
            OM_S
        </S>
        <S>
            OP.|->
            <S>
                OP.Cyc ( BE.LoopBnd 200 )
                <S>
                    OP.|->
                    <S>
                        OP.|->
                        <S>
                            OP.|->
                            <S>
                                OP.|->
                                OM_V
                                <S>
                                    OP.? ( BE.SCI 25 )
                                    OM_SS
                                    OM_SS.Rand
                                </S>
                            </S>
                            OM_D
                        </S> OMS.IterCounter
                    </S> OMS.TimeCounter
                </S>
            </S>
            <S> OP.OSend (send_1) OM_R.Tabu </S>
        </S>
    </S>
</S>
};

solver1 := solver
{
    cStrategy: St_g01;
    oModule: OM_S.GolombValid, OM_V.1ChangeOrd, OM_SS.FirstImpr, OM_D.AlwImpr;
};

solverc1 := solver
{
    cStrategy: St_g01COMM;
    oModule: OM_S.GolombValid, OM_V.1ChangeOrd, OM_SS.FirstImpr, OM_D.AlwImpr;
    oChannel: OCh.Sol_Last;
};

solverc2 := solver
{
    cStrategy: St_g01COMM;
    oModule: OM_S.GolombValid, OM_V.1ChangeOrd, OM_SS.FirstImpr, OM_D.AlwImpr;
    oChannel: OCh.Sol_Last;
};

connections:

[solver1]2;
[solverc1.send_1, solverc2.send_1] OP.<> [solverc1.Ch1, solverc2.Ch1]2;

