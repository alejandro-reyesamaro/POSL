runs=2

files_results=(golomb_no_tabu.pres golomb_tabu.pres golomb_comm_1-1.pres golomb_comm_1-n.pres);
strs=(./golomb_no_tabu.posl ./golomb_tabu.posl ./golomb_comm_1-1.posl ./golomb_comm_1-n.posl); 

o=(8 10);
l=(34 55);
cores=40

for (( r = 0; r < 2; r++ ))
do
	for (( j = 0; j < 4; j++ ))
	do
		echo "----GOLOMB ${o[r]}-${l[r]} : $cores  -----" >> ${files_results[j]}
		for (( i = 0; i < $runs; i++ ))
		do
			mpiexec.mpich2 -np $cores ./bin/POSL golomb ${o[r]}-${l[r]} ${strs[j]} >> ${files_results[j]}	
		done
		echo "----END-----" >> ${files_results[j]}
		#w >> ${files_results[j]}
		echo "   " >> ${files_results[j]}
	done
done

