runs=50

files_results=(golomb_8-34_sec.pres golomb_10-55_sec.pres golomb_11-72_sec.pres);
strs_no_tabu=(./golomb8_no_tabu.posl ./golomb10_no_tabu.posl ./golomb11_no_tabu.posl); 
strs_tabu=(./golomb8_tabu.posl ./golomb10_tabu.posl ./golomb11_tabu.posl);

cores=1


echo "----GOLOMB 11-72 : $cores  ----- (no tabu)" >> golomb_11-72_sec.pres
for (( i = 0; i < $runs; i++ ))
do
	mpiexec.mpich2 -np $cores ./bin/POSL golomb 11-72 golomb11_no_tabu.posl >> golomb_11-72_sec.pres
done
echo "----END-----" >> golomb_11-72_sec.pres
#w >> golomb_11-72_sec.pres
echo "   " >> golomb_11-72_sec.pres


echo "----GOLOMB 11-72 : $cores  -----" >> golomb_11-72_sec.pres
for (( i = 0; i < $runs; i++ ))
do
	mpiexec.mpich2 -np $cores ./bin/POSL golomb 11-72 golomb11_tabu.posl >> golomb_11-72_sec.pres
done
echo "----END-----" >> golomb_11-72_sec.pres
#w >> golomb_11-72_sec.pres
echo "   " >> golomb_11-72_sec.pres
