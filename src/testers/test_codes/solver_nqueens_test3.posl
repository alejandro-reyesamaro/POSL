St_sender := cStrategy
    oModule: OM_S, OM_V, OM_SS, OM_D;
{
<S>
    OP.Cyc (BE.LoopBnd 20)
    <S>
        OP.|-> 	OM_S
                <S>
                    OP.Cyc ( BE.OR ( BE.LoopBnd 4000, BE.SCI 200 ) )
                    <S>
                        OP.|->
                        <S>
                            OP.|->
                            <S>
                                OP.|->
                                <S>
                                    OP.|->
                                    OM_V
                                    OM_SS
                                </S>
                                <S>
                                    OP.? (BE.LoopBnd 10)
                                    OM_D
                                    <S> OP.OSend (send_1) OM_D </S>
                                </S>
                            </S> OMS.IterCounter
                        </S> OMS.TimeCounter
                    </S>
                </S>
    </S>
</S>
};

St_sr := cStrategy
    oModule: OM_S, OM_V, OM_SS, OM_D;
    oChannel: OCh;
{
<S>
    OP.Cyc (BE.LoopBnd 20)
    <S>
        OP.|-> 	OM_S
                <S>
                    OP.Cyc ( BE.OR ( BE.LoopBnd 4000, BE.SCI 200 ) )
                    <S>
                        OP.|->
                        <S>
                            OP.|->
                            <S>
                                OP.|->
                                <S>
                                    OP.|->
                                    OM_V
                                    OM_SS
                                </S>
                                <S>
                                    OP.? (BE.LoopBnd 10)
                                    OM_D
                                    <S>
                                        OP.Min
                                        <S> OP.OSend (send_1) OM_D </S>
                                        OCh (Ch1)
                                    </S>
                                </S>
                            </S> OMS.IterCounter
                        </S> OMS.TimeCounter
                    </S>
                </S>
    </S>
</S>
};

St_receiver := cStrategy
    oModule: OM_S, OM_V, OM_SS, OM_D;
    oChannel: OCh;
{
<S>
        OP.|->
        OM_S
        <S>
          OP.Cyc ( BE.LoopBnd 4000 )
          <S>
                   OP.|->
                   <S>
                       OP.|->
                       <S>
                           OP.|-> 	OM_V
                                   <S>
                                       OP.|->
                                       OM_SS
                                       <S>
                                           OP.? (BE.LoopBnd 5)
                                           OM_D
                                           <S> OP.Min OM_D OCh (Ch1) </S>
                                       </S>
                                   </S>
                       </S> OMS.IterCounter
                   </S> OMS.TimeCounter
          </S>
        </S>
</S>
};

s_solver1 := solver
{
        cStrategy: St_sender;
        oModule: OM_S.PermRand, OM_V.ASR(0-1000), OM_SS.FirstImpr, OM_D.AlwImpr;
};

s_solver2 := solver
{
        cStrategy: St_sender;
        oModule: OM_S.PermRand, OM_V.ASR(1000-2000), OM_SS.FirstImpr, OM_D.AlwImpr;
};

s_solver3 := solver
{
        cStrategy: St_sender;
        oModule: OM_S.PermRand, OM_V.ASR(2000-3000), OM_SS.FirstImpr, OM_D.AlwImpr;
};

s_solver4 := solver
{
        cStrategy: St_sender;
        oModule: OM_S.PermRand, OM_V.ASR(3000-4000), OM_SS.FirstImpr, OM_D.AlwImpr;
};

sr_solver1 := solver
{
        cStrategy: St_sr;
        oModule: OM_S.PermRand, OM_V.ASR(0-2000), OM_SS.FirstImpr, OM_D.AlwImpr;
        oChannel: OCh.Sol_Last;
};

sr_solver2 := solver
{
        cStrategy: St_sr;
        oModule: OM_S.PermRand, OM_V.ASR(2000-4000), OM_SS.FirstImpr, OM_D.AlwImpr;
        oChannel: OCh.Sol_Last;
};

r_solver := solver
{
        cStrategy: St_receiver;
        oModule: OM_S.PermRand, OM_V.AS, OM_SS.FirstImpr, OM_D.AlwImpr;
        oChannel: OCh.Sol_Last;
};

connections:

[s_solver1.send_1, s_solver2.send_1] OP.#-> [sr_solver1.Ch1];
[s_solver3.send_1, s_solver4.send_1] OP.#-> [sr_solver2.Ch1];
[sr_solver1.send_1, sr_solver2.send_1] OP.#-> [r_solver.Ch1];
