runs=50

files_results=(golfers11_50.pres golfers11_25.pres);
strs=(./g_1-1_50.posl ./g_1-1_25.posl);

p=5
g=3
w=7
cores=40

for (( j = 0; j < 2; j++ ))
do	
		echo "----$p-$g-$w : ${cores[j]}  -----" >> ${files_results[j]}
		for (( i = 0; i < $runs; i++ ))
		do
			mpiexec.mpich2 -np $cores ./bin/POSL golfers $p-$g-$w ${strs[j]} >> ${files_results[j]}	
		done
		echo "----END-----" >> ${files_results[j]}
		#w >> ${files_results[j]}
		echo "   " >> ${files_results[j]}
done

