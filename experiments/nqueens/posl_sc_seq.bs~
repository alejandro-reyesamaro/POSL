runs=1

files_result="nqueens_par.pres";
str="./nq.posl";

n=(250 500 1000 3000 6000);
cores=40

for (( nq = 0; nq < 5; nq++ )) do
for (( j = 0; j < 8; j++ ))
do	
		echo "----${n[nq]} : $cores  -----" >> $files_result
		for (( i = 0; i < $runs; i++ ))
		do
			mpiexec.mpich2 -np $cores ./bin/POSL nqueens ${n[nq]} $str >> $files_result
		done
		echo "----END-----" >> $files_result
		echo "   " >> $files_result
done
done
