St_sr_a := cStrategy
	oModule: OM_S, OM_R, OM_V, OM_SS, OM_D;
	oChannel: OCh;
{
<S>
    OP.|->
    OM_S
    <S>
        OP.Cyc ( BE.LoopBnd 2000000 )
        <S>
            OP.|->
            OM_R
            <S>
                OP.Cyc (BE.LoopBnd 3)
                <S>
                    OP.|->
                    <S>
                        OP.|->
                        <S>
                            OP.|->
                            <S> OP.|-> OM_V OM_SS </S>
									 <S>
										OP.? (BE.LoopBnd 12)
										<S> OP.OSend (send_1) OM_D </S> 
										<S> OP.Min OM_D OCh (Ch1) </S>
									 </S>
                        </S> OMS.IterCounter
                    </S> OMS.TimeCounter
                </S>
            </S>
        </S>
    </S>
</S>
};

std_solver := solver(20)
{
        cStrategy: St_sr_a;
        oModule: OM_S.PermRand, OM_R.Daniel, OM_V.AS, OM_SS.FirstImpr, OM_D.AlwImpr;
        oChannel: OCh.Sol_Last;
};

comp_solver := solver(20)
{
        cStrategy: St_sr_a;
        oModule: OM_S.PermRand, OM_R.Daniel, OM_V.ASP(2), OM_SS.FirstImpr, OM_D.AlwImpr;
        oChannel: OCh.Sol_Last;
};

connections:

[std_solver.send_1] OP.--> [comp_solver.Ch1]20;
[comp_solver.send_1] OP.--> [std_solver.Ch1]20;

