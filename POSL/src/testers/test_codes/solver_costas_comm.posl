St_rca000_a := cStrategy
        oModule: OM_S, OM_R, OM_V, OM_SS, OM_D;
        oChannel: OCh;
{
<S>
    OP.|->
    OM_S
    <S>
        OP.Cyc ( BE.LoopBnd 24000 )
        <S>
            OP.|->
            OM_R
            <S>
                OP.Cyc (BE.LoopBnd 3)
                <S>
                    OP.|->
                    <S>
                        OP.|->
                        <S>
                            OP.|->
                            <S> OP.|-> OM_V OM_SS </S>
                            <S> OP.Min OM_D OCh (Ch1) </S>
                        </S> OMS.IterCounter
                    </S> OMS.TimeCounter
                </S>
            </S>
        </S>
    </S>
</S>
};

