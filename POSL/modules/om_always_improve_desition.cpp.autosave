#include "om_always_improve_desition.h"

     #include "data/strategy_search_in_neighborhood.h"
     #include "data/best_improvement_v_search_state.h"

OM_AlwaysImproveDesition::OM_AlwaysImproveDesition()
{
}

ComputationData * OM_AlwaysImproveDesition::execute(PSP *psp, ComputationData * input)
{
    StrategySearchInNeighborhood * s = new StrategySearchInNeighborhood(new BestImprovement_VSearchState());
    return s->select(psp->GetBenchmark(), (Neighborhood *)input);
}